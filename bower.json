    "main": "dist/pwstrength-foundation.js",
    "dependencies": {
        "jquery": ">=1.7.0",
        "foundation": ">=2.0.0"
    },
    "moduleType": [
        "globals"
    ],
    "keywords": [
        "jquery",
        "foundation",
        "password",
        "strength",
        "meter"
    ],
    "license": "MIT",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "spec",
        "examples",
        "AUTHORS.txt",
        "CHANGES.md",
        "Gruntfile.js",
        "package.json",
        "pwstrength-foundation.jquery.json"
    ]
}
